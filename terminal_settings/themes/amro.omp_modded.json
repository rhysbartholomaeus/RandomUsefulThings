{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "#cd5e42",
          "properties": {
            "prefix": "\u0000",
            "root_icon": "\u26A1",
            "postfix": "\u0000"
          }
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "#95FFA4",
          "properties": {
            "prefix": "\u0000",
            "template": "{{.UserName}}"
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#0CA0D8",
          "properties": {
            "style": "full",
            "prefix": " \uf07b ",
            "folder_separator_icon": "/"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "powerline_symbol": "\uE0B0",
          "foreground": "#14A5AE",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "prefix": "",
            "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \uF692 {{ .StashCount }}{{ end }}"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "time",
          "style": "diamond",
          "foreground": "#141228",
          "background": "#CBE1E7",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#CD4277",
          "properties": {
            "prefix": "",
            "text": "\u0023"
          }
        }
      ]
    }
  ]
}